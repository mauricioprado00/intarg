					<title>Listado</title>
				</action>
				<action method="addItem">
					<link_url>resultado_esperado/addEdit</link_url>
					<text>Agregar</text>
					<title>Agregar</title>
				</action>
			</block>
		</reference>
	</default>
	<datalist_admin_resultado_esperado>
<!--			output_filters="utf8_encode"   -->
